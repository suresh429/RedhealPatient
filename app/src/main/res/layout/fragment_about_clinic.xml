<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.medoske.www.redhealpatient.Fragments.AboutClinicFragment">

    <!-- TODO: Update blank fragment layout -->

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@android:color/white"
        android:padding="3dp">

        <!--<ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/mapList"/>-->


        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardUseCompatPadding="true"            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:padding="10dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageView
                        android:id="@+id/imageView10"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        app:srcCompat="@drawable/ic_action_fee" />

                    <TextView
                        android:id="@+id/textFee"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="23dp"
                        android:layout_marginTop="6dp"
                        android:layout_toEndOf="@+id/imageView10"
                        android:text="INR 500 counsultation Fee" />

                    <View
                        android:id="@+id/textView36"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignStart="@+id/textFee"
                        android:layout_below="@+id/imageView10"
                        android:layout_marginTop="10dp"
                        android:background="#d8d7d7"
                        android:text="TextView" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageView
                        android:id="@+id/imageView105"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        app:srcCompat="@drawable/ic_action_location" />

                    <TextView
                        android:id="@+id/textClinicName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/imageView105"
                        android:text="TextView" />

                    <TextView
                        android:id="@+id/textAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignStart="@+id/textClinicName"
                        android:layout_below="@+id/textClinicName"
                        android:text="TextView"
                        android:textSize="8sp"
                        android:layout_marginTop="5dp"/>

                    <WebView
                        android:id="@+id/webViewMap"
                        android:layout_width="match_parent"
                        android:layout_height="90dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignStart="@+id/textAddress"
                        android:layout_below="@+id/textAddress"
                        android:layout_marginTop="10dp" />

                    <View
                        android:id="@+id/textView37"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignStart="@+id/webViewMap"
                        android:layout_below="@+id/textViewMore"
                        android:layout_marginTop="10dp"
                        android:background="#d8d7d7"
                        android:text="TextView" />

                    <TextView
                        android:id="@+id/textViewMore"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_below="@+id/webViewMap"
                        android:text="View More"
                        android:textColor="@color/colorPrimaryDark"
                        android:layout_marginTop="5dp"
                        android:padding="5dp"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageView
                        android:id="@+id/imageView104"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        app:srcCompat="@drawable/ic_action_time" />

                    <TextView
                        android:id="@+id/Timings"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/imageView104"
                        android:text="Timings" />

                    <TextView
                        android:id="@+id/textTimings"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/imageView104"
                        android:layout_below="@+id/Timings"
                        android:text="TextView"
                        android:textSize="12sp"
                        android:layout_marginTop="5dp"/>

                    <View
                        android:id="@+id/textView38"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignStart="@+id/textTimings"
                        android:layout_below="@+id/textTimings"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="10dp"
                        android:background="#d8d7d7"
                        android:text="TextView" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp">

                    <ImageView
                        android:id="@+id/imageView101"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        app:srcCompat="@drawable/ic_action_clinic" />

                    <ImageView
                        android:id="@+id/imageView16"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="23dp"
                        android:layout_toEndOf="@+id/imageView101"
                        app:srcCompat="@drawable/ic_action_education" />

                    <ImageView
                        android:id="@+id/imageView17"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/imageView16"
                        app:srcCompat="@drawable/ic_action_education" />

                    <ImageView
                        android:id="@+id/imageView18"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="5dp"
                        android:layout_toEndOf="@+id/imageView17"
                        app:srcCompat="@drawable/ic_action_education" />

                    <View
                        android:id="@+id/textView39"
                        android:layout_width="wrap_content"
                        android:layout_height="1dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignStart="@+id/imageView16"
                        android:layout_below="@+id/imageView16"
                        android:layout_marginTop="10dp"
                        android:background="#d8d7d7" />

                </RelativeLayout>


            </LinearLayout>
        </android.support.v7.widget.CardView>


    </android.support.v4.widget.NestedScrollView>


</RelativeLayout>
